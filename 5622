#include <stdio.h>
#include <string.h>

main() {
	char num[15];
	int t = 0;
	gets(num);

	for (int i = 'A'; i <= 'Z'; i++) {
		for (int j = 0; j < strlen(num); j++) {
			if (num[j] == i)
				t += dial(i);
		}
	}
	t += strlen(num);
	printf("%d", t);
}

int dial(int n) {
	switch (n) {
	case 'A':
	case 'B':
	case 'C':
		return 2;
	case 'D':
	case 'E':
	case 'F':
		return 3;
	case 'G':
	case 'H':
	case 'I':
		return 4;
	case 'J':
	case 'K':
	case 'L':
		return 5;
	case 'M':
	case 'N':
	case 'O':
		return 6;
	case 'P':
	case 'Q':
	case 'R':
	case 'S':
		return 7;
	case 'T':
	case 'U':
	case 'V':
		return 8;
	case 'W':
	case 'X':
	case 'Y':
	case 'Z':
		return 9;
	}
 }
