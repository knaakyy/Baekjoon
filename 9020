#include <stdio.h>
int is_prime(int n)
{
	if(n<2) return 0;
	
	for(int i=2;i<n;i++)
		if(n%i==0)
			return 0;
	
	return 1;
}
int main(void)
{
	int N, C, i;
	scanf("%d",&C);
	
	while(C--)
	{
		scanf("%d",&N);
		
		for(i=N/2;i>=2;i--)
			if(is_prime(i)&&is_prime(N-i))
			{
				printf("%d %d\n",i, N - i);
				break;
			}
	}
	
	return 0;
}
