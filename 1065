#include <stdio.h>

int AP(int a, int b, int c) {
	return((a - b) == (b - c)) ? 1 : 0;
}

int main() {
	int N, i;
	int a, b, c = 0;
	int cnt = 0;
	
	scanf("%d", &N);
	if (N < 100) {
		printf("%d", N);
	}
	else {
		for (i = 100; i <= N; i++) {
			a = i / 100;
			b = i / 10 % 10;
			c = i % 10;

			if (AP(a, b, c) == 1) {
				cnt++;
			}
		}
		printf("%d", cnt + 99);
	}
	return 0;
}
